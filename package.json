{
  "name": "icare",
  "version": "0.1.0",
  "private": true,
  "description": "symptom checker capstone project for team 5 in 2022sp CSE 5914",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AAAblaze/iCare.git"
  },
  "keywords": [
    "symptom",
    "checker",
    "capstone"
  ],
  "author": "Hanyuan Cui, Kangxuan Ye, Kunal Arya, Patrick Sheeran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AAAblaze/iCare/issues"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11",
    "eslint-config-google": "^0.14.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": { "modules": true }
    },
    "plugins": [
      "vue"
    ],
    "rules": {
      "indent": ["error", 4],
      "linebreak-style": 0,
      "quotes": ["error", "single"],
      "semi": ["error", "never"]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
